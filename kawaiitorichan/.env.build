# Build-time environment variables
# These prevent database connection attempts during build
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
SKIP_BUILD_STATIC_GENERATION=true
SKIP_DB_PUSH=true
SKIP_DB_SEED=true
DATABASE_URI=postgresql://build:build@db:5432/build
PAYLOAD_SECRET=build_time_secret_will_be_replaced_at_runtime_minimum_32_chars
NEXT_PUBLIC_SERVER_URL=http://localhost:3000
PAYLOAD_CONFIG_PATH=dist/payload.config.js